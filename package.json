  "private": true,
  "scripts": {
    "postinstall": "pnpm seed:generate",
    "dev": "pnpm clean:cache && next dev --turbopack",
    "build": "next build --turbopack",
    "postbuild": "next-sitemap && pnpm api:docs",
    "start": "next start",
    "format": "prettier \"**/*.{js,jsx,ts,tsx}\" --write",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:reset": "supabase db reset",
    "db:seed": "tsx supabase/seed/seed.ts > supabase/seed.sql",
    "seed:sync": "snaplet-seed sync --config supabase/seed/seed.config.ts",
    "seed:generate": "snaplet-seed generate --config supabase/seed/seed.config.ts",
    "clean:cache": "rm -rf .next node_modules/.cache",
    "clean:all": "rm -rf .next node_modules package-lock.json pnpm-lock.yaml node_modules/.cache && pnpm install",
    "api:docs": "tsx scripts/generate-openapi.ts & npx @redocly/cli build-docs public/openapi.json --output=public/api.html",
    "prepare": "husky",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
